<h1>Sing up</h1>

<%=  form_with model: @user, url: sign_up_path do |form| %>
    <%= form.label :email %>
    <%= form.text_field :email %>
    <%= form.password_field :password, placeholder: "password" %>
    <%= form.password_field :password_confirmation, placeholder: "password" %>
    <%= form.submit "Sign up" %>
<% end %>